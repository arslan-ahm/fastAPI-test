{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "API_HOST": "0.0.0.0",
    "API_PORT": "8000"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  },
  "regions": ["iad1"]
}